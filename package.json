    "node-gyp": "^9.0.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "prompts": "^2.4.2",
    "puppeteer": "^14.1.2",
    "rimraf": "^3.0.2",
    "socket.io": "^4.5.1",
    "typescript": "^4.7.2",
    "uuid": "^8.3.2",
    "winston": "^3.7.2",
