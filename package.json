{
  "name": "was-v2",
  "version": "0.1.0",
  "description": "Walking and Sponsoring donation and walker tracking + postprocessing",
  "main": "backend.js",
  "scripts": {
    "tsc": "tsc -b",
    "tsc-clean": "rm ./backend*; rm -rf ./includes; rm -rf ./static/js",
    "log-clean": "rm -rf ./log",
    "clean": "npm run tsc-clean; npm run log-clean;",
    "run": "tsc -b; node backend.js",
    "ssl": "mkdir keys; openssl req -x509 -nodes -newkey rsa:2048 -keyout key.pem -out cert.pem -days 30"
  },
